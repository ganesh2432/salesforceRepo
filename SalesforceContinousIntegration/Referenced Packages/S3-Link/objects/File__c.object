    <validationRules>
        <fullName>File_Extension_Validation</fullName>
        <active>false</active>
        <description>File Extension cannot be updated once file get created.</description>
        <errorConditionFormula>AND(NOT(ISNEW()), ISCHANGED(  Extension__c ))</errorConditionFormula>
        <errorDisplayField>Extension__c</errorDisplayField>
        <errorMessage>Extension cannot be updated manually. Its a system generated field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>File_Name_Validation</fullName>
        <active>true</active>
        <description>The file name you provided cannot contain special characters like /, &lt;, &gt;, ? or &amp;.</description>
        <errorConditionFormula>OR(CONTAINS( Name , &quot;/&quot;), CONTAINS( Name , &quot;&gt;&quot;), CONTAINS( Name , &quot;&lt;&quot;), CONTAINS( Name , &quot;?&quot;), CONTAINS( Name , &quot;&amp;&quot;))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>The file name you provided cannot contain special characters like /, &lt;, &gt;, ? or &amp;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>File_Size_Validation</fullName>
        <active>true</active>
        <description>File Size cannot be updated once file get created.</description>
        <errorConditionFormula>AND(NOT(ISNEW()), ISCHANGED(  Size__c ))</errorConditionFormula>
        <errorDisplayField>Size__c</errorDisplayField>
        <errorMessage>Size cannot be updated manually. Its a system generated field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Folder_Validation</fullName>
        <active>true</active>
        <description>Folder cannot updated manually.</description>
        <errorConditionFormula>AND(NOT(System_Update__c), NOT(ISNEW()), ISCHANGED( Folder__c ))</errorConditionFormula>
        <errorDisplayField>Folder__c</errorDisplayField>
        <errorMessage>Folder cannot be updated manually. Its a system generated field.</errorMessage>
    </validationRules>
